{
  "private": true,
  "name": "vitepress-theme-teek-docs-template",
  "version": "1.0.0",
  "description": "vitepress-theme-teek 文档模板",
  "type": "module",
  "scripts": {
    "docs:dev": "concurrently \"node watch-files.js\" \"vitepress dev docs\"",
    "docs:dev:docker": "concurrently \"node watch-files.js\" \"vitepress dev docs --host 0.0.0.0 --port 8848\"",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:clean": "rm -rf docs/.vitepress/cache docs/.vitepress/dist",
    "cache:clear": "./clear-cache.sh",
    "new": "./new-post.sh",
    "test:auto-index": "./test-auto-index.sh"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "echarts": "^5.6.0",
    "sass": "^1.89.1",
    "vitepress": "^1.6.3",
    "vitepress-plugin-llms": "^1.7.3"
  },
  "dependencies": {
    "vitepress-theme-teek": "^1.5.4",
    "vue": "^3.5.16"
  }
}
